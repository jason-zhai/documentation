}


a {
  text-decoration: none;

}
a:hover {
  text-decoration: none;
}

#mobile-hamburger {
  cursor: pointer;
  border-radius: 3px;
  display: block;
  position: fixed;
  z-index: 15;
  top: 10px;
  left: 0;
  margin-left: 10px;
  width: 50px;
  height: 40px;
  -webkit-transition: left 0.2s;
  -o-transition: left 0.2s;
  transition: left 0.2s;
}
#mobile-hamburger div {
  width: 60%;
  height: 6px;
  margin: 0 auto;
  margin-bottom: 4px;
  background: #354052;
}
#mobile-hamburger div:first-child {
  margin-top: 9px;
}
@media (min-width: 992px) {
  #mobile-hamburger {

    display: none;
  }
}

.solutions #mobile-hamburger div,
.platform #mobile-hamburger div,
.company #mobile-hamburger div,
.partners #mobile-hamburger div,
.careers #mobile-hamburger div {
  background: white;
  border: 1px solid #ddd;
}

#side-menu {

	 font-family: Lato, "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 14px;
  line-height: 1.42857;
  position: fixed;
  left: -80%;
  z-index: 15;
  height: 100%;
  width: 80%;
  background-color: #354052;
  text-align: center;
  font-weight: 300;
  padding-bottom: 50px;
  overflow: auto;
  -webkit-transition: width 0.2s, left 0.2s;
  -o-transition: width 0.2s, left 0.2s;
  transition: width 0.2s, left 0.2s;
}
@media (min-width: 992px) {
  #side-menu {
    left: 0;
    width: 80px;
    overflow: hidden;
  }
  #side-menu nav {
    width: 80px;
  }
  #side-menu:hover {
    width: 200px;
  }
  #side-menu:hover nav {
    width: 200px;
  }

}
@media (max-width: 992px) {
  #side-menu {
    width: 0px;
    left: 0;
  }
.side_menu{
    width: 400px  !important;
}
#mobile-hamburger{
left:0px;
}
