					    elementSide.style.bottom = ( wScrollCurrent + wHeight - 4226 ) + 'px';
                if(wScrollCurrent < 3235) {
					    elementSide.style.bottom = '10px';
					    elementSide.style.top = '53px';
                }
                if(wScrollCurrent < 130) { //element slides in near the top
                    element.style.top = ( elTop > 0 ? 0 : elTop ) + 'px';
                    elementSide.style.top = ( elTop > 0 ? 182 : 182 - wScrollCurrent ) + 'px';
                }
            }
			else if( wScrollDiff < 0 ) {// scrolled down
				if( wScrollCurrent + wHeight >= dHeight - elHeight )  {// scrolled to the very bottom; element slides in
					//element.style.top = ( ( elTop = wScrollCurrent + wHeight - dHeight ) < 0 ? elTop : 0 ) + 'px';
                }
				else {// scrolled down; element slides out
					element.style.top = ( Math.abs( elTop ) > elHeight ? -elHeight : elTop ) + 'px';
					elementSide.style.top = ( wScrollCurrent < 130 ? (  182 - wScrollCurrent ) : 60  ) + 'px';
                    //console.log(Math.abs( elTop ) > elHeight ? -elHeight : elTop);

                }
			}
            if(wScrollCurrent > 3235) { //locking the main right menu
					    elementSide.style.bottom = ( wScrollCurrent + wHeight - 4226 ) + 'px';
					    elementSide.style.top = '-' + ( wScrollCurrent + wHeight - 4226 ) + 'px';
                    }
			wScrollBefore = wScrollCurrent;
		});
	}( document, window, 0 ));
